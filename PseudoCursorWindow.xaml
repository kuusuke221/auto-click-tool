<Window x:Class="auto_click_tool.PseudoCursorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        WindowStyle="None" AllowsTransparency="True" Background="Transparent"
        Width="18" Height="18" Topmost="True" ShowInTaskbar="False"
        ShowActivated="False" Focusable="False" IsHitTestVisible="False" ResizeMode="NoResize">
    <Grid IsHitTestVisible="False">
        <Ellipse x:Name="ellipsePseudoCursor" Width="14" Height="14" Fill="#FFFF0000" Stroke="Black" StrokeThickness="1" Opacity="0.9"/>
    </Grid>
</Window>